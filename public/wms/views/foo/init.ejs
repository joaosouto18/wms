<h2>foos</h2>
<table cellspacing='0px'>
	<thead>
	<tr>
		<% for(var attr in Wms.Models.Foo.attributes){%>
			<% if(attr == 'id') continue;%>
			<th><%= attr%> </th>	 
		<%}%>
		<th>Options</th>
	</tr>
	</thead>
	<tbody>
		<%= $.View('//wms/views/foo/list',{foos: foos})%>
	</tbody>
</table>
<h2>New foo</h2>
<form>
	<% for(var attr in Wms.Models.Foo.attributes){ %>
		<% if(attr == 'id') continue;%>
		<div>
			<label><%= attr %> </label><br />
			<input type="text" value="" name="<%= attr %>"/>
		</div>
	<%}%>
	<input type='submit' value='Create'/>
</form>